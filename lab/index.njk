---
layout: base.njk
title: The Lab
---

<div class="lab-grid">
  {%- for project in collections.lab -%}
    <div class="project-entry">
      <a href="{{ project.data.github_url }}" class="retro-link">
        {{ project.data.title }}
      </a>
      <ul class="lesson-list">
        {%- for lesson in project.data.lessons -%}
          <li>{{ lesson }}</li>
        {%- endfor -%}
      </ul>
    </div>
  {%- endfor -%}
</div>